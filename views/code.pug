extends layout

block content
  .container
    h1= title
    .row
      .col-md-8.col-md-offset-2
        form#codeform(action="#")
          label(for=code) Put your code here
          textarea#code(type="text" class="md-textarea form-control")
          br
          button.btn.btn-dark(type="submit") Compile


  script(src="//cdn.jsdelivr.net/npm/codemirror@5.41.0/lib/codemirror.min.js")
  script(src="//cdnjs.cloudflare.com/ajax/libs/codemirror/5.41.0/mode/clike/clike.min.js")
  script(src="https://code.jquery.com/jquery-3.3.1.min.js")
  script(src='/javascripts/bundle.js')
    
